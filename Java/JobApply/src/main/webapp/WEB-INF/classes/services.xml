<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="cityService" class="cd.itcast.ssh.service.impl.CityServiceImpl">
		<property name="dao" ref="cityDAO" />
	</bean>
	<bean id="tradeService" class="cd.itcast.ssh.service.impl.TradeServiceImpl">
		<property name="dao" ref="tradeDAO" />
	</bean>
	<bean id="salaryLevelService" class="cd.itcast.ssh.service.impl.SalaryLevelServiceImpl">
		<property name="dao" ref="salaryLevelDAO" />
	</bean>
	<bean id="companyTypeService" class="cd.itcast.ssh.service.impl.CompanyTypeServiceImpl">
		<property name="dao" ref="companyTypeDAO" />
	</bean>
	<bean id="companyService" class="cd.itcast.ssh.service.impl.CompanyServiceImpl">
		<property name="dao" ref="companyDAO" />
	</bean>
	<bean id="jobApplyService" class="cd.itcast.ssh.service.impl.JobApplyServiceImpl">
		<property name="dao" ref="jobApplyDAO" />
	</bean>
</beans>