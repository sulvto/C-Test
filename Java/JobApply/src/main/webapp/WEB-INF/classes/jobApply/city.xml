<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="city" namespace="/" extends="struts-default">
		<action name="city_*" method="{1}" class="cityAction">
			<result name="list">/WEB-INF/jsps/city/list.jsp</result>
			<result name="input">/WEB-INF/jsps/city/edit.jsp</result>
			<result name="success" type="redirectAction">city_list</result>
		</action>
	</package>
</struts>
