<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="trade" namespace="/" extends="struts-default">
		<action name="trade_*" method="{1}" class="tradeAction">
			<result name="list">/WEB-INF/jsps/trade/list.jsp</result>
			<result name="input">/WEB-INF/jsps/trade/edit.jsp</result>
			<result name="success" type="redirectAction">trade_list</result>
		</action>
	</package>
</struts>
