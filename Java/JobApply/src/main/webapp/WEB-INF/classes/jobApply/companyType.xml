<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="companyType" namespace="/" extends="struts-default">
		<action name="companyType_*" method="{1}" class="companyTypeAction">
			<result name="list">/WEB-INF/jsps/companyType/list.jsp</result>
			<result name="input">/WEB-INF/jsps/companyType/edit.jsp</result>
			<result name="success" type="redirectAction">companyType_list</result>
		</action>
	</package>
</struts>
