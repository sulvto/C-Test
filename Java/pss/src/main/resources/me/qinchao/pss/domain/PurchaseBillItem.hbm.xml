<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="me.qinchao.pss.domain">

	<class name="PurchaseBillItem">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="price" />
		<property name="num" />
		<property name="amount" />
		<property name="descs" />
		<!-- private PurchaseBill bill;// 组合关系,非空 -->
		<many-to-one name="bill" column="bill_id" not-null="true" />
		<!-- private Product product;// 多对一,非空 -->
		<many-to-one name="product" column="product_id" not-null="true" />


	</class>



</hibernate-mapping>
