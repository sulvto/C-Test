<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cd.qin.pss.domain">

	<class name="me.qinchao.pss.domain.ProductStock">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="storeNum" />
		<property name="price" />
		<property name="amount" />
		<property name="incomeDate" />
	<property name="outcomeDate" />
		<property name="warning" />
		<property name="topNum" />
		<property name="bottomNum" />
		<many-to-one name="depot" column="depot_id" />
		<many-to-one name="product" column="product_id" />
	</class>

</hibernate-mapping>
