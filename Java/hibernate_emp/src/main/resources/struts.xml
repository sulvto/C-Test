<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false"/>
    <constant name="struts.devMode" value="true"/>
    <package name="action" namespace="/" extends="struts-default">
        <action name="emp_*" class="me.qinchao.web.EmployeeAction" method="{1}">
            <result>/WEB-INF/views/list.jsp</result>
            <result name="delete" type="redirectAction">emp</result>
            <result name="save" type="redirectAction">emp</result>
            <result name="input">/WEB-INF/views/edit.jsp</result>
        </action>
    </package>
</struts>
